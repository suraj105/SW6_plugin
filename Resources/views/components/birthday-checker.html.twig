{% block birthday_checker_tab %}
    <div>
        <h2>Birthday Checker</h2>
        <p><strong>First Name:</strong> {{ customer.first_name }}</p>
        <p><strong>Last Name:</strong> {{ customer.last_name }}</p>
        <p><strong>Birthday:</strong> {{ customer.birthday | date('Y-m-d') }}</p>
    </div>
{% endblock %}
